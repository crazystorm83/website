{
  "name": "tsx",
  "version": "1.0.0",
  "packageManager": "pnpm@10.20.0",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/server/action.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pnpm --filter './src' build",
    "build:primitive_types": "pnpm --filter './src/01.primitive_types' build",
    "build:foundation": "pnpm --filter './src/02.foundation' build",
    "build:browser_foundation": "pnpm --filter './src/05.browser_foundation' build",
    "build:browser_infra": "pnpm --filter './src/10.browser_infra' build",
    "build:browser_node": "pnpm --filter './src/20.browser_node' build",
    "build:browser": "pnpm --filter './src/90.browser' build",
    "build:server": "pnpm --filter './src/server' build",
    "build:bundle": "pnpm --filter './src' build:bundle",
    "build:bundle:primitive_types": "pnpm --filter './src/01.primitive_types' build:bundle",
    "build:bundle:foundation": "pnpm --filter './src/02.foundation' build:bundle",
    "build:bundle:browser_foundation": "pnpm --filter './src/05.browser_foundation' build:bundle",
    "build:bundle:browser_infra": "pnpm --filter './src/10.browser_infra' build:bundle",
    "build:bundle:browser_node": "pnpm --filter './src/20.browser_node' build:bundle",
    "build:bundle:browser": "pnpm --filter './src/90.browser' build:bundle",
    "build:bundle:server": "pnpm --filter './src/server' build:bundle",
    "watch": "esbuild src/*.jsx --bundle --outfile=bundle.js --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "esbuild": "^0.25.11",
    "eslint": "^9.39.1",
    "express": "^5.1.0",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.7.2",
    "tsx": "^4.20.6",
    "turbo": "^2.6.1"
  },
  "dependencies": {
    "path": "^0.12.7",
    "vite": "^7.2.4",
    "y-websocket": "^3.0.0",
    "yjs": "^13.6.27"
